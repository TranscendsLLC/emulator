<reader>
	<readerclassname>org.rifidi.emulator.reader.thingmagic.module.ThingMagicReaderModule</readerclassname>
	<maxgpos>0</maxgpos>
	<maxgpis>0</maxgpis>
	<description>Emulator for the ThingMagic Mercury 4 Reader</description>
	<maxantennas>4</maxantennas>
	<property>
		<required>1</required>
		<name>rql_address</name>
		<display>rql ip address</display>
		<tooltip>The address the reader should be reachable through telnet</tooltip>
		<defaultvalue>127.0.0.1:8080</defaultvalue>		
		<validatorclassname>IpAddressValidator</validatorclassname>
	</property>
</reader>