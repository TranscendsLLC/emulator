<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="ScriptingReaderManager" class="org.rifidi.emulator.scripting.ReaderManager">
		<property name="moduleFactoryList" ref="readerFactories"></property>
	</bean>
	
	<bean id="SpringTest" class="org.rifidi.emulator.scripting.testing.SpringTest">
		<property name="scriptingReaderManager" ref="ScriptingReaderManager"></property>
	</bean>

</beans>