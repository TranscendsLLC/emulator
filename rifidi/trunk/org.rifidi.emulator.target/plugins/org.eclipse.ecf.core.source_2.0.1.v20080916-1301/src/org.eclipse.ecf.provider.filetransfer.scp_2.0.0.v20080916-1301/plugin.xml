<plugin>
   <extension
         point="org.eclipse.ecf.provider.filetransfer.sendFileTransferProtocolFactory">
      <sendFileTransferProtocolFactory
            class="org.eclipse.ecf.internal.provider.filetransfer.scp.ScpSendFileTransferFactory"
            protocol="scp">
